{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "PIC18 assembly",
	"patterns": [
		{
			"include": "#instructions"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#directives"
		},
		{
			"name": "comment.line",
			"match": ";.*$"
		},
		{
			"comment": "Label",
			"name": "entity.name.function",
			"match": "^[ \\t]*([A-Za-z_\\?][\\dA-Za-z_\\?\\$]*):"
		},
		{
			"include": "#includeDefine"
		},
		{
			"include": "#macro"
		}
	],
	"repository": {
		"instructions": {
			"patterns": [
				{
					"name": "entity.name.tag.byte-oriented.add",
					"match": "^[ \\t]*([Aa][Dd][Dd][Ww][Ff]|[Aa][Dd][Dd][Ww][Ff][Cc])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.subtract",
					"match": "^[ \\t]*([Ss][Uu][Bb][Ff][Ww][Bb]|[Ss][Uu][Bb][Ww][Ff]|[Ss][Uu][Bb][Ww][Ff][Bb])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.comparison",
					"match": "^[ \\t]*([Cc][Pp][Ff][Ss][Ee][Qq]|[Cc][Pp][Ff][Ss][Gg][Tt]|[Cc][Pp][Ff][Ss][Ll][Tt])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.move",
					"match": "^[ \\t]*([Mm][Oo][Vv][Ff]|[Mm][Oo][Vv][Ff][Ff]|[Mm][Oo][Vv][Ww][Ff])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.increment",
					"match": "^[ \\t]*([Ii][Nn][Cc][Ff]|[Ii][Nn][Cc][Ff][Ss][Zz]|[Ii][Nn][Ff][Ss][Nn][Zz])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.decrement",
					"match": "^[ \\t]*([Dd][Ee][Cc][Ff]|[Dd][Ee][Cc][Ff][Ss][Zz]|[Dd][Cc][Ff][Ss][Nn][Zz])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.bitwise",
					"match": "^[ \\t]*([Aa][Nn][Dd][Ww][Ff]|[Ii][Oo][Rr][Ww][Ff]|[Xx][Oo][Rr][Ww][Ff]|[Rr][Ll][Cc][Ff]|[Rr][Ll][Nn][Cc][Ff]|[Rr][Rr][Cc][Ff]|[Rr][Rr][Nn][Cc][Ff])\\b"
				},
				{
					"name": "entity.name.tag.byte-oriented.other",
					"match": "^[ \\t]*([Cc][Ll][Rr][Ff]|[Ss][Ee][Tt][Ff]|[Cc][Oo][Mm][Ff]|[Nn][Ee][Gg][Ff]|[Mm][Uu][Ll][Ww][Ff]|[Ss][Ww][Aa][Pp][Ff]|[Tt][Ss][Tt][Ff][Ss][Zz])\\b"
				},
				{
					"name": "entity.name.tag.bit-oriented",
					"match": "^[ \\t]*([Bb][Cc][Ff]|[Bb][Ss][Ff]|[Bb][Tt][Ff][Ss][Cc]|[Bb][Tt][Ff][Ss][Ss]|[Bb][Tt][Gg])\\b"
				},
				{
					"name": "entity.name.tag.control.branch",
					"match": "^[ \\t]*([Bb][Cc]|[Bb][Nn]|[Bb][Nn][Cc]|[Bb][Nn][Nn]|[Bb][Nn][Oo][Vv]|[Bb][Nn][Zz]|[Bb][Oo][Vv]|[Bb][Rr][Aa]|[Bb][Zz])\\b"
				},
				{
					"name": "entity.name.tag.stack",
					"match": "^[ \\t]*([Cc][Aa][Ll][Ll]|[Rr][Cc][Aa][Ll][Ll]|[Pp][Oo][Pp]|[Pp][Uu][Ss][Hh]|[Rr][Ee][Tt][Uu][Rr][Nn]|[Rr][Ee][Tt][Ll][Ww])\\b"
				},
				{
					"name": "entity.name.tag.other",
					"match": "^[ \\t]*([Gg][Oo][Tt][Oo]|[Cc][Ll][Rr][Ww][Dd][Tt]|[Dd][Aa][Ww]|[Nn][Oo][Pp]|[Rr][Ee][Ss][Ee][Tt]|[Rr][Ee][Tt][Ff][Ii][Ee]|[Ss][Ll][Ee][Ee][Pp])\\b"
				},
				{
					"name": "entity.name.tag.literal.wreg",
					"match": "^[ \\t]*([Aa][Dd][Dd][Ll][Ww]|[Aa][Nn][Dd][Ll][Ww]|[Ii][Oo][Rr][Ll][Ww]|[Mm][Oo][Vv][Ll][Ww]|[Mm][Uu][Ll][Ll][Ww]|[Rr][Ee][Tt][Ll][Ww]|[Ss][Uu][Bb][Ll][Ww]|[Xx][Oo][Rr][Ll][Ww])\\b"
				},
				{
					"name": "entity.name.tag.literal.other",
					"match": "^[ \\t]*([Ll][Ff][Ss][Rr]|[Mm][Oo][Vv][Ll][Bb])\\b"
				},
				{
					"name": "entity.name.tag.memory",
					"match": "^[ \\t]*[Tt][Bb][Ll]([Rr][Dd]|[Ww][Tt])(\\*(\\+|\\-)?|\\+\\*)[ \\t]*$"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.single",
					"match": "'.*'"
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.binary",
					"match": "\\b[01]+B\\b"
				},
				{
					"name": "constant.numeric.octal",
					"match": "\\b([0-7])+(O|o|Q|q)\\b"
				},
				{
					"name": "constant.numeric.decimal",
					"match": "\\b\\d+(D|d)?\\b"
				},
				{
					"name": "constant.numeric.hexadecimal",
					"match": "\\b0([\\da-fA-F]+[Hh]|[xX][\\da-fA-F]+)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "constant.language.current_pc",
					"match": "\\s\\$(\\s|$)"
				},
				{
					"name": "constant.language.address_byte",
					"match": "\\b([Ll][Oo][Ww]( [Hh][Ii][Gg][Hh][Ww][Oo][Rr][Dd])?|[Hh][Ii][Gg][Hh])\\b"
				},
				{
					"name": "constant.language.shift",
					"match": "\\b([Ss][Hh][Ll]|[Ss][Hh][Rr])\\b"
				},
				{
					"name": "constant.language.comparison",
					"match": "\\b([Gg][Ee]|[Gg][Tt]|[Ll][Ee]|[Ll][Tt]|[Ee][Qq]|[Nn][Ee])\\b"
				},
				{
					"name": "constant.language.and",
					"match": "\\b[Aa][Nn][Dd]\\b"
				}
			]
		},
		"directives": {
			"patterns": [
				{
					"name": "keyword.control.preprocessor",
					"match": "^[ \\t]*#([Uu][Nn][Dd][Ee][Ff]|[Ii][Ff]|[Ii][Ff][Dd][Ee][Ff]|[Ii][Ff][Nn][Dd][Ee][Ff]|[Ee][Ll][Ss][Ee]|[Ee][Nn][Dd][Ii][Ff])\\b"
				},
				{
					"name": "meta.preprocessor.directive",
					"match": "\\b([Aa][Ll][Ii][Gg][Nn]|[Aa][Ss][Mm][Oo][Pp][Tt]|[Bb][Aa][Nn][Kk][Ss][Ee][Ll]|[Cc][Aa][Ll][Ll][Ss][Tt][Aa][Cc][Kk]|[Cc][Oo][Nn][Dd]|[Nn][Oo][Cc][Oo][Nn][Dd]|[Cc][Oo][Nn][Ff][Ii][Gg]|[Dd][Bb]|[Dd][Ww]|[Dd][Ss]|[Dd][Aa][Bb][Ss]|[Dd][Ll][Aa][Bb][Ss]|[Dd][Dd][Ww]|[Ee][Ll][Ss][Ee]|[Ee][Ll][Ss][Ii][Ff]|[Ee][Nn][Dd][Ii][Ff]|[Ee][Nn][Dd]|[Ee][Nn][Dd][Mm]|[Ee][Qq][Uu]|[Ee][Rr][Rr][Oo][Rr]|[Ee][Xx][Pp][Aa][Nn][Dd]|[Nn][Oo][Ee][Xx][Pp][Aa][Nn][Dd]|[Ee][Xx][Tt][Rr][Nn]|[Ff][Nn][Aa][Dd][Dd][Rr]|[Ff][Nn][Aa][Rr][Gg]|[Ff][Nn][Bb][Rr][Ee][Aa][Kk]|[Ff][Nn][Cc][Aa][Ll][Ll]|[Ff][Nn][Cc][Oo][Nn][Ff]|[Ff][Nn][Ii][Nn][Dd][Ii][Rr]|[Ff][Nn][Ss][Ii][Zz][Ee]|[Ff][Nn][Rr][Oo][Oo][Tt]|[Gg][Ll][Oo][Bb][Aa][Ll]|[Ii][Ff]|[Ii][Nn][Cc][Ll][Uu][Dd][Ee]|[Ii][Rr][Pp]|[Ii][Rr][Pp][Cc]|[Ll][Ii][Ss][Tt]|[Nn][Oo][Ll][Ii][Ss][Tt]|[Ll][Oo][Cc][Aa][Ll]|[Mm][Aa][Cc][Rr][Oo]|[Mm][Ee][Ss][Ss][Gg]|[Oo][Rr][Gg]|[Pp][Aa][Gg][Ee][Ll][Ee][Nn]|[Pp][Aa][Gg][Ee][Ss][Ee][Ll]|[Pp][Aa][Gg][Ee][Ww][Ii][Dd][Tt][Hh]|[Pp][Rr][Oo][Cc][Ee][Ss][Ss][Oo][Rr]|[Pp][Ss][Ee][Cc][Tt]|[Rr][Aa][Dd][Ii][Xx]|[Rr][Ee][Pp][Tt]|[Ss][Ee][Tt]|[Ss][Ii][Gg][Nn][Aa][Tt]|[Ss][Uu][Bb][Tt][Ii][Tt][Ll][Ee]|[Tt][Ii][Tt][Ll][Ee])\\b"
				},
				{
					"name": "meta.preprocessor.directive.mpasm_only",
					"match": "\\b(__[Bb][Aa][Dd][Rr][Aa][Mm]|[Bb][Aa][Nn][Kk][Ii][Ss][Ee][Ll]|[Cc][Bb][Ll][Oo][Cc][Kk]|[Cc][Oo][Dd][Ee]|__[Cc][Oo][Nn][Ff][Ii][Gg]|[Cc][Oo][Nn][Ss][Tt][Aa][Nn][Tt]|[Dd][Aa]|[Dd][Aa][Tt][Aa]|[Dd][Ee]|[Dd][Tt]|[Ee][Nn][Dd][Cc]|[Ee][Nn][Dd][Ww]|[Ee][Rr][Rr][Oo][Rr][Ll][Ee][Vv][Ee][Ll]|[Ee][Xx][Ii][Tt][Mm]|[Ee][Xx][Tt][Ee][Rr][Nn]|[Ff][Ii][Ll][Ll]|[Ii][Dd][Aa][Tt][Aa]|__[Ii][Dd][Ll][Oo][Cc][Ss]|[Ii][Ff][Dd][Ee][Ff]|[Ii][Ff][Nn][Dd][Ee][Ff]|__[Mm][Aa][Xx][Rr][Aa][Mm]|[Pp][Aa][Gg][Ee]|[Rr][Ee][Ss]|[Ss][Pp][Aa][Cc][Ee]|[Ss][Uu][Bb][Tt][Ii][Tt][Ll]|[Uu][Dd][Aa][Tt][Aa]|[Uu][Dd][Aa][Tt][Aa]_[Aa][Cc][Ss]|[Uu][Dd][Aa][Tt][Aa]_[Oo][Vv][Rr]|[Uu][Dd][Aa][Tt][Aa]_[Ss][Hh][Rr]|[Vv][Aa][Rr][Ii][Aa][Bb][Ll][Ee]|[Ww][Hh][Ii][Ll][Ee])\\b"
				},
				{
					"name": "meta.preprocessor.banksel",
					"match": "\\b[Bb][Aa][Nn][Kk][Ss][Ee][Ll](?=\\b|\\()"
				},
				{
					"name": "meta.preprocessor.bankmask",
					"match": "\\b[Bb][Aa][Nn][Kk][Mm][Aa][Ss][Kk](?=\\b|\\()"
				},
				{
					"include": "#include"
				},
				{
					"include": "#define"
				},
				{
					"include": "#equ"
				},
				{
					"include": "#registerOrWreg"
				},
				{
					"include": "#acsOrBsr"
				}
			]
		},
		"include": {
			"begin": "^[ \\t]*#([Ii][Nn][Cc][Ll][Uu][Dd][Ee])\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.control.preprocessor.include"
				}
			},
			"end": "$",
			"patterns": [
				{
					"match": "[ \\t]+<[^<]+>([ \\t]|$)",
					"name": "meta.preprocessor.string"
				},
				{
					"match": "[ \\t]+\"[^\"]+\"([ \\t]|$)",
					"name": "meta.preprocessor.string"
				}
			]
		},
		"define": {
			"begin": "^[ \\t]*#([Dd][Ee][Ff][Ii][Nn][Ee])\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.control.preprocessor.include"
				}
			},
			"end": "[ \\t]+\\S+\\b",
			"endCaptures": {
				"0": {
					"name": "meta.preprocessor"
				}
			}
		},
		"equ": {
			"match": "^[ \\t]*([A-Za-z_\\?][\\dA-Za-z_\\?\\$]*)[ \\t]+(?=[Ee][Qq][Uu])",
			"name": "variable.name"
		},
		"registerOrWreg": {
			"match": "(?<=([Aa][Dd][Dd][Ww][Ff]|[Aa][Dd][Dd][Ww][Ff][Cc]|[Aa][Nn][Dd][Ww][Ff]|[Cc][Oo][Mm][Ff]|[Dd][Ee][Cc][Ff]|[Dd][Ee][Cc][Ff][Ss][Zz]|[Dd][Cc][Ff][Ss][Nn][Zz]|[Ii][Nn][Cc][Ff]|[Ii][Nn][Cc][Ff][Ss][Zz]|[Ii][Nn][Ff][Ss][Nn][Zz]|[Ii][Oo][Rr][Ww][Ff]|[Mm][Oo][Vv][Ff]|[Rr][Ll][Cc][Ff]|[Rr][Ll][Nn][Cc][Ff]|[Rr][Rr][Cc][Ff]|[Rr][Rr][Nn][Cc][Ff]|[Ss][Uu][Bb][Ff][Ww][Bb]|[Ss][Uu][Bb][Ww][Ff]|[Ss][Uu][Bb][Ww][Ff][Bb]|[Ss][Ww][Aa][Pp][Ff]|[Xx][Oo][Rr][Ww][Ff])[^\\n]*,[ \\t]*)[WwFf](?=($|[, \\t;]))",
			"name": "constant.character.escape"
		},
		"acsOrBsr": {
			"match": "(?<=([Aa][Dd][Dd][Ww][Ff]|[Aa][Dd][Dd][Ww][Ff][Cc]|[Aa][Nn][Dd][Ww][Ff]|[Bb][Cc][Ff]|[Bb][Ss][Ff]|[Bb][Tt][Ff][Ss][Cc]|[Bb][Tt][Ff][Ss][Ss]|[Bb][Tt][Gg]|[Cc][Ll][Rr][Ff]|[Cc][Oo][Mm][Ff]|[Cc][Pp][Ff][Ss][Ee][Qq]|[Cc][Pp][Ff][Ss][Gg][Tt]|[Cc][Pp][Ff][Ss][Ll][Tt]|[Dd][Ee][Cc][Ff]|[Dd][Ee][Cc][Ff][Ss][Zz]|[Dd][Cc][Ff][Ss][Nn][Zz]|[Ii][Nn][Cc][Ff]|[Ii][Nn][Cc][Ff][Ss][Zz]|[Ii][Nn][Ff][Ss][Nn][Zz]|[Ii][Oo][Rr][Ww][Ff]|[Mm][Oo][Vv][Ff]|[Mm][Oo][Vv][Ww][Ff]|[Mm][Uu][Ll][Ww][Ff]|[Nn][Ee][Gg][Ff]|[Rr][Ll][Cc][Ff]|[Rr][Ll][Nn][Cc][Ff]|[Rr][Rr][Cc][Ff]|[Rr][Rr][Nn][Cc][Ff]|[Ss][Ee][Tt][Ff]|[Ss][Uu][Bb][Ff][Ww][Bb]|[Ss][Uu][Bb][Ww][Ff]|[Ss][Uu][Bb][Ww][Ff][Bb]|[Ss][Ww][Aa][Pp][Ff]|[Tt][Ss][Tt][Ff][Ss][Zz]|[Xx][Oo][Rr][Ww][Ff])[^\\n]*,[ \\t]*)[BbAaCc](?=($|[, \\t;]))",
			"name": "constant.character.escape"
		},
